{
  "name": "weare",
  "version": "1.0.0",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src&&set WEBPACK_ENV=development&&webpack -w",
    "build:assets": "cd src&&set WEBPACK_ENV=production&&webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.gonfig.js'",
    "copyAll": "shx cp -R src/static build && shx cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "shx rm -rf build",
    "start": "node build/init.js",
    "presetart": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.7.5",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.21.3",
    "heroku": "^7.39.1",
    "heroku-config": "^1.5.4",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "babel-cli": "^6.26.0"
  }
}